/* pages/apartment/index/index.wxss */
.box-1 {
    background: url('https://images.gongyuabc.com/image/icon/search_bg.png') left top no-repeat;
    background-size: cover;
}
.box-1 .wrap {
    padding: 15px 20px;
}
.box-1 .map {
    width: 48%;
    height: 118px;
    float: left;
    background: url('https://images.gongyuabc.com/image/icon/zuobiao.png') center center no-repeat;
    background-size: cover;
    border-radius: 2px;
}
.box-1 .commute {
    width: 48%;
    height: 118px;
    float: right;
    border-radius: 2px;
    background: url('https://images.gongyuabc.com/image/icon/tongqin.png') center center no-repeat;
    background-size: cover;
}
.box-1 .map .inner, .box-1 .commute .inner {
    padding: 10px;
}
.box-1 .title {
    color: #4A4A4A;
    font-size: 17px;
    margin-bottom: 5px;
    font-weight: 500;
}
.box-1 .desc {
    color: #9B9B9B;
    font-size: 14px;
}
.box-2 .wrap {
    padding: 15px;
}
.box-2 .title {
    margin-bottom: 10px;
    font-size: 17px;
    color: #4A4A4A
}
.box-2 .container {
    background:rgba(255,255,255,1);
    box-shadow:0px 0px 5px 0px rgba(223,222,222,0.5);
    border-radius:3px;
}
.box-2 .container .inner {
    padding: 10px;
}
.box-2 .container .box-title {
    margin-bottom: 30px;
    font-size: 14px;
    color: #4A4A4A;
    line-height: 30px;
}
.box-2 .box-title .icon-book {
    display: inline-block;
    width: 30px;
    height: 30px;
    margin-right: 15px;
    background: url('https://images.gongyuabc.com/image/iconnew/book.png') center center no-repeat;
    background-size: cover;
}
.box-2 .item {
    margin-bottom: 20px;
}
.box-2 .item .selected {
    color: #FFCF11 !important;
}
.box-2 .item .icon {
    margin-right: 30px;
    text-align: left;
    line-height: 60px;
}
.box-2 .item icon {
    font-size: 20px;
    color: #9B9B9B;
}
.box-2 .item .headimg {
    width: 60px;
    height: 60px;
    border-radius: 60px;
    margin-right: 15px;
}
.box-2 .item .headimg image {
    width: 100%;
    height: 100%;
    border-radius: 60px;
}
.box-2 .item .info-box {
    margin-top: 8px;
}
.box-2 .item .house-type {
    font-size: 14px;
    color: #4A4A4A;
    margin-bottom: 2px;
    max-width: 120px;
}
.box-2 .item .date {
    max-width: 120px;
    font-size: 11px;
    color: #8C8B8C;
}
.box-2 .item .status-box {
    margin-top: 8px;
    margin-left: 10px;
}
.box-2 .item .status-box .time {
    font-size: 14px;
    color: #9B9B9B
}
.box-2 .item .status-box .status {
    font-size: 11px;
    color: #9B9B9B;
}
.box-2 .btn-box {
    margin-top: 50px;
}
.box-2 .btn-box .need {
    font-size: 14px;
    color: #FFC000;
    margin-bottom: 8px;
    text-align: center;
}
.box-2 .btn-box .tip {
    font-size: 11px;
    text-align: center;
    color: #DFDEDE;
}
.box-2 .btn-box .group {
    margin-bottom: 8px;
}
.box-2 .group .btn {
    width: 177px;
    height: 46px;
    line-height: 46px;
    text-align: center;
    font-size: 14px;
    color: #000;
    margin: 0 auto;
    background: rgba(255,207,17,1);
    box-shadow: 0px 0px 4px 0px rgba(219,218,218,1);
    border-radius: 23px;
}
.box-2 .no-trip .img {
    width: 100px;
    height: 64px;
    margin: 0 auto;
}
.box-2 .no-trip image {
    width: 100%;
    height: 100%;
}
.box-2 .no-trip .tip {
    font-size: 11px;
    margin-top: 12px;
    color: #BBB9BC;
    text-align: center;
}
.box-3 {
    width: 92%;
    position: fixed;
    left: 4%;
    top: 100px;
    z-index: 600;
    background-color: #fff;
    border-radius: 5px;
}
.box-3 .inner {
    padding: 15px;
}
.box-3 .title {
    font-size: 17px;
    color: #4A4A4A;
    text-align: center;
    margin-bottom: 20px;
}
.box-3 .title icon {
    display: inline-block;
    width: 22px;
    height: 20px;
    margin-right: 8px;
    background: url('https://images.gongyuabc.com/image/icon/select.png') center center no-repeat;
    background-size: cover;
}
.box-3 .select {
    margin-bottom: 30px;
}
.box-3 .select .item {
    display: flex;
}
.box-3 .select text {
    max-width: 120px;
    font-size: 13px;
    color: #4A4A4A;
}
.box-3 .select .icon-down-arrow {
    font-size: 14px;
    color: #4A4A4A;
    position: relative;
    top: -4px;
    left: 3px;
}
.box-3 .check {
    padding: 4px 0;
}
.box-3 .check-title {
    font-size: 14px;
    color: #000;
    margin-right: 25px;
}
.box-3 .check .item {
    font-size: 12px;
    color: #4A4A4A;
    padding: 2px 8px;
    float: left;
    margin-right: 8px;
    background-color: #EEEEEE;
    border-radius: 11px;
}
.box-3 .check .selected {
    background-color: #FFCF11;
}
.box-3 .btn {
    width: 291px;
    height: 46px;
    background: #FFCF11;
    box-shadow: 0px 0px 4px 0px rgba(219,218,218,1);
    border-radius: 23px;
    margin: 50px auto 5px auto;
    text-align: center;
    font-size: 14px;
    color: #000;
    line-height: 46px;
}
.box-4 {
    position: fixed;
    right: 6px;
    bottom: 10px;
    z-index: 9;
    width: 50px;
    height: 50px;
}
.box-4 button {
    width: 50px;
    height: 50px;
    background: url('https://images.gongyuabc.com/image/iconnew/liaokefu.png') center center no-repeat;
    background-size: cover;
}
.box-5 {
    background-color: #fff;
    width: 80%;
    position: fixed;
    left: 10%;
    top: 150px;
    z-index: 600;
    border-radius: 5px;
    border-top: 2px solid #FFCF11;
}
.box-5 .close {
    width: 30px;
    height: 30px;
    position: fixed;
    top: 110px;
    left: 47%;
    z-index: 600;
    text-align: center;
    line-height: 30px;
}
.box-5 .close icon {
    font-size: 28px;
    color: #fff;
}
.box-5 .inner {
    padding: 20px;
}
.box-5 .mobile .weui-flex, .box-5 .code .weui-flex {
    padding: 10px 0 0px 0;
    border-bottom: 1px solid #efefef;
}
.box-5 .inner .title {
    font-size: 14px;
    color: #ccc;
    line-height: 30px;
    margin-right: 6px;
}
.box-5 .inner input {
    display: inline-block;
    font-size: 13px;
    color: #666;
    line-height: 30px;
    height: 30px;
}
.box-5 .inner .get-code {
    padding: 2px 5px;
    border-radius: 5px;
    background-color: #FFCF11;
    text-align: center;
    line-height: 26px;
    color: #666;
    font-size: 12px;
}
.box-5 .btn {
    font-size: 13px;
    color: #666;
    background-color: #FFCF11;
    text-align: center;
    line-height: 30px;
    margin-top: 20px;
    border-radius: 3px;
}