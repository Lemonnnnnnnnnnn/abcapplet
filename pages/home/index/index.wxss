/* pages/home/index/index.wxss */
::-webkit-scrollbar {
   width: 0;
   height: 0;
   color: transparent;
}
.wrap {
    padding: 12px 12px 0 12px;
}
.box-1 {
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 100;
    background-color: #fff;
}
.box-1 .search-inner {
    padding: 0 12px 12px 12px;
}
.box-1 .search-box {
    height: 37px;
    background:rgba(243, 242, 243, 1);
    border-radius: 19px;
}
.box-1 .city {
    width: 70px;
    line-height: 37px;
    margin-left: 15px;
}
.box-1 .city text {
    font-size: 14px;
    color: #4A4A4A;
}
.box-1 .city icon {
    color: #4A4A4A;
    font-size: 12px;
    margin-left: 5px;
}
.box-1 .search {
    margin-left: 35px;
    height: 37px;
    line-height: 37px;
    color: #9B9B9B;
}
.box-1 .search icon {
    display: inline-block;
    margin-right: 10px;
}
.box-2 {
    margin-top: 49px;
}
.box-2 .scroll-view {
    width: 100%;
    height: 221px;
    overflow: hidden;
    white-space: nowrap;
}
.box-2 .item {
    width: 313px;
    margin-right: 10px;
    display: inline-block;
}
.box-2 .first-item {
    margin-left: 10px;
}
.box-2 .item .img {
    width: 313px;
    height: 172px;
    border-radius: 5px;
}
.box-2 .img image {
    width: 100%;
    height: 100%;
    border-radius: 5px;
}
.box-2 .item .info {
    margin-top: 5px;
}
.box-2 .info .title {
    font-size: 16px;
    color: #4A4A4A;
}
.box-2 .info .tip {
    font-size: 11px;
    color: #9B9B9B;
}
.box-2 .dots {
    text-align: center;
    padding: 0 12px;
}
.box-2 .dots icon {
    display: inline-block;
    width: 12px;
    height: 2px;
    background-color: #D8D8D8;
    margin: 0 3px;
}
.box-2 .dots .selected {
    width:4px;
    height:4px;
    border: 1px solid #FFCF11;
    border-radius: 4px;
    background-color: #fff;
}
.box-3 .title {
    padding: 3px 12px 8px 12px;
    font-size: 17px;
    font-weight: 500;
    color:#4A4A4A;
    line-height: 24px;
}
.box-3 .scroll-view {
    width: 100%;
    height: 174px;
    overflow: hidden;
    white-space: nowrap;
}
.box-3 .item {
    width: 131px;
    height: 168px;
    margin-right: 12px;
    display: inline-block;
}
.box-3 .first-item {
    margin-left: 12px;
}
.box-3 .item image {
    width: 100%;
    height: 100%;
    border-radius: 5px;
}
.box-4 .title {
    margin-bottom: 8px;
    font-size: 17px;
    font-weight: 500;
    color:#4A4A4A;
    line-height: 24px;
}
.box-4 .scroll-view {
    width: 100%;
    height: 181px;
    overflow: hidden;
    white-space: nowrap;
}
.box-4 .item {
    width: 306px;
    height: 175px;
    margin-right: 12px;
