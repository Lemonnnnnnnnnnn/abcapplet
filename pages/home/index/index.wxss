}
.hide {
    display: none;
}
.box-7 {
    width:100%;
    height:39px;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 10;
    background-color: #fff;
}
.box-7 .select {
    margin-bottom: 6px;
}
.box-7 .select text {
    font-size: 13px;
    color: #4A4A4A;
}
.box-7 .select .icon-down-arrow {
    font-size: 14px;
    color: #4A4A4A;
    position: relative;
    top: -4px;
    left: 3px;
}
.box-8 .content {
    position: fixed;
    width: 80%;
    left: 10%;
    top: 80px;
    z-index: 1100;
}
.box-8 .content image {
    width: 100%;
    display: block;
}
.box-8 .city {
    background: #fff;
    border-radius: 0 0 8px 8px;
}
.box-8 .city .item {
    text-align: center;
    line-height: 32px;
    color: #3a3a3a;
    border-bottom: 1px solid #e7e7e7;
    font-size: 15px;
}
.box-8 .city .item:last-child {
    border: none;
}
.box-8 .city .title {
    text-align: center;
    font-size: 16px;
    line-height: 36px;
    border-bottom: 1px solid #e7e7e7;
    position: relative;
}
.box-8 .city .title:before {
    content: '';
    width: 16px;
    height: 3px;
    background: #ffcf11;
    position: absolute;
    left: 18%;
    top: 18px;
    border-radius: 6px;
}
.box-8 .city .title:after {
    content: '';
    width: 16px;
    height: 3px;
    background: #ffcf11;
    position: absolute;
    right: 18%;
    top: 18px;
    border-radius: 6px;
}
.box-8 .city-mask {
    z-index: 1000;
}
/** 加载更多 **/
.load-more {
    position: relative;
    padding: 30px 0 20px;
    text-align: center;
    color: #888;
    height: 22px;
}
.load-more .hr {
    position: absolute;
    top: 40px;
    width: 60%;
    height: 1px;
    background-color: #888;
    margin-left: 20%;
}
.load-more .text-container {
    display: inline-block;
    height: 22px;
    width: 40%;
    margin: 0 auto;
    background-color: #fff;
    z-index: 1;
    position: absolute;
    left: 30%;
}
.load-more .text:before, .load-morer .text:after {
    content: "";
    display: inline-block;
    width: 4px;
    height: 4px;
    border-radius: 50%;
    background-color: #888;
    position: relative;
    right: 5px;
    top: -3px;
}
.load-more .text:after {
    left: 5px;
}
.load-more .no-data {
    margin: 20px auto 30px auto;
}